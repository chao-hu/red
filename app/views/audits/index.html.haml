.row
  .col-md-4.col-lg-2
    = render :partial => "settings/setting_left"
  .col-md-8.col-lg-10
    .box.box-danger
      .box-header.with-border
        .box-title Audit
        .box-tools.pull-right
          %button.btn.btn-box-tool
            = fa_icon(:minus)
        .box-body
          = render :partial => "audits/list", locals: { audits: @audits }

      = paginate @audits
